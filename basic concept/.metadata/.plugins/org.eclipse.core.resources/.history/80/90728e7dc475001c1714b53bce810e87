package PatternProblem;

public class pattern5 {

	public static void main(String[] args) {
//	  --> first type	
//		patternFive(5);
//		patternfive(5);
		
		
		//--->> Second type
		
		for(int row=1;row<=9;row++) {
			if(row>=1 && row<=5) {
				for(int col=1;col<=row;col++) {
					System.out.print("*");
				}
				System.out.println();
			}
			else {
				for(int col=1;col<9-row;col++) {
					System.out.print("*");
				}
				System.out.println();
			}
		}
		
		
		
	}
//   --->> first type
//	static void patternFive(int n) {
//		for(int row=1;row<=5;row++) {
//			for(int col=1;col<=row;col++) {
//				System.out.print("*");
//			}
//			System.out.println();
//		}
//	}
//	
//	 static void patternfive(int n) {
//       for(int row =1;row<n;row++) {
//    	   for(int col=1;col<=n-row;col++) {
//    		   System.out.print("*");
//    	   }
//    	   System.out.println();
//       }
//	}
	
	
}
